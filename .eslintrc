{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": ["prettier", "airbnb-base"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "off",
        "indent": ["error", 4],
        "comma-dangle": "off",
        "no-restricted-syntax": "off",
        "operator-linebreak": "off",
        "linebreak-style": "off",
        "consistent-return": "off",
        "implicit-arrow-linebreak": "off",
        "function-paren-newline": "off",
        "object-curly-newline": "off"
    }
}
